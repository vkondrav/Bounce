    //number of times the bottom is hit
    n = floor(xs/ys)
    
    var proceed = true;
    
    var t = instance_number(obj_block);
    
    //for every block object        
    for(var k = 0; k &lt; t; k++){
        inst = instance_find(obj_block, k);
        
        //for every final_frame in the blovk object array
        for(var l = 0; l &lt; array_length_1d(inst.final_frames); l++){
            
            //for every
            for(var i = 0; i &lt; amount; i++){
                
                for(var j = 1; j &lt;= n; j++){
                    if(inst.final_frames[l] == ys*j + spawn_rate*i){                    
